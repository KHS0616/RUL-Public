<Window x:Class="RUL_Program.GateWay"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RUL_Program"
        mc:Ignorable="d"
        Title="GateWay" Height="450" Width="800">
    <Grid>
        <Frame x:Name="frContent" Content="Frame" HorizontalAlignment="Left" Height="388" Margin="0,31,0,0" VerticalAlignment="Top" Width="792" Background="#FFDEB9B9" NavigationUIVisibility="Hidden"/>
        <StackPanel HorizontalAlignment="Left" Height="31" VerticalAlignment="Top" Width="792" Orientation="Horizontal">
            <Button x:Name="btnGatewayMain" Content="Main" Click="btnGateway_Click"/>
            <Button x:Name="btnGateway001" Content="1호기" Width="75" Click="btnGateway_Click"/>
            <Button x:Name="btnGateway002" Content="2호기" Width="75" Click="btnGateway_Click"/>
            <Button x:Name="btnGateway003" Content="3호기" Width="75" Click="btnGateway_Click"/>
            <Button x:Name="btnGateway004" Content="4호기" Width="75" Click="btnGateway_Click"/>
        </StackPanel>

    </Grid>
</Window>
